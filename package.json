{
  "name": "sp-digital-menu",
  "version": "1.0.0",
  "description": "A simple project for a digital menu for a fast-food restaurant",
  "author": "David Almeida <contato.davidalmeida@outlook.com>",
  "main": "./index.html",
  "keywords": [
    "fast-food",
    "digital",
    "menu",
    "html"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/emeraldknight-sp/template-html.git"
  },
  "scripts": {
    "start": "npx tailwindcss -i ./src/styles/style.css -o ./src/styles/output.css --watch",
    "lint": "eslint \"src/**/*.{html, js}\" --quiet",
    "format": "prettier --write \"src/**/*.{html,js}\""
  },
  "devDependencies": {
    "eslint": "8.57.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-html": "^8.1.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.6"
  },
  "license": "MIT"
}
